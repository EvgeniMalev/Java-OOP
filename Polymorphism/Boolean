public class Boolean { 
  

    public int boolean(boolean x, boolean y) 
    { 
        return (x && y); 
    } 
  

    public int boolean(boolean x, boolean y) 
    { 
        return (x || y); 
    } 
  
    public double boolean(boolean x, boolean y, boolean z, boolean p) 
    { 
        return ((x && y)||(x || y)); 
    } 
  
    public static void main(String args[]) 
    { 
        Sum s = new Sum(); 
        System.out.println(s.sum(true, false)); 
        System.out.println(s.sum(true, true)); 
        System.out.println(s.sum(false, false, false, false)); 
    } 
} 
